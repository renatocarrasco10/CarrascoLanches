@model IEnumerable<CarrascoLanches.Models.Lanche>

<div>
    <h2>Todos os Lanches</h2>

</div>

@foreach (var lanche in Model)
{
    <div>
        <p><h3>@lanche.Nome</h3>
        <img src="@lanche.ImagemUrl" />
         <h4>@Html.DisplayNameFor(model => model.Preco) : @lanche.Preco.ToString("c")</h4></p>
    </div>
}